;-------------------------random module------------------------------------------
randomlike:
if (&lt;random region&gt; == 1) then 
    prints &quot;\n\t       random region module:\n&quot;, imatrix 
    iquant  =   1 - &lt;quantity events&gt;
    kxstartR init imatrix * iquant 
    while (kxstartR &lt; imatrix) do  

        kxmin   =   &lt;minimumX&gt; * imatrix
        kxmax   =   &lt;maximumX&gt; * imatrix
        kymin   =   &lt;minimumY&gt; * imatrix
        kymax   =   &lt;maximumY&gt; * imatrix
        kX      random  kxmin, kxmax
        kY      random  kymin, kymax

        kmaxw    =   &lt;WeightR max&gt;                                              ; weight  
        kminw    =   &lt;WeightR min&gt;
        kw  random  kminw, kmaxw

        kxstartR += 1
        prints &quot;\n\t\tX\tY\tW\n&quot;                                   ; to screen
        prints &quot;\t\t--\t--\t--\n&quot;                                  ; to screen
        printsk &quot;\t\t%d\t%d\t%d\n&quot;, kX, kY, kw
        fprintks &quot;&lt;filename&gt;_&lt;matrix&gt;.matrxT&quot;, &quot;%d  %d  %d\n&quot;, kX, kY, kw       ;-imatrix
    od
endif
;-------------------------------------------------------------------------------

;----------------------waves module---------------------------------------------

if (&lt;waves module&gt; == 1) then
    ky init 0
    kx init 0

    prints &quot;\n\t       waves module:\n&quot;
    while kx &lt; imatrix do
        kx += 1
        krfreq  loopxseg   kr/imatrix, 0, 0, &lt;frequencymovement_slower&gt;, 0
        krdepth loopxseg   kr/imatrix, 0, 0, &lt;depthmovement_less&gt;, 0
        kres   k_lfo7  &lt;frequency max&gt;*krfreq, &lt;depth max&gt;*krdepth, &lt;wave&gt; 
        kres    = (kres + 1)*.5

        ky += 1
        kres    *= (imatrix)-1
        kres    += kx -(imatrix/2)
        kres    limit   kres, 1, imatrix

        kmaxww    =   &lt;Weight maxWaves&gt;                                         ; weight  
        kminww    =   &lt;Weight minWaves&gt;
        kw  random  kminww, kmaxww 
        kw  =   int(kw)

        prints &quot;\n\t\tX\tY\tW\tmirror?\n&quot;                                    ; to screen
        prints &quot;\t\t--\t--\t--\t-------\n&quot;                                  ; to screen
        printsk &quot;\t\t%d\t%d\t%d\n&quot;, kres, kx, kw
        fprintks &quot;&lt;filename&gt;_&lt;matrix&gt;.matrxT&quot;, &quot;%d  %d  %d\n&quot;, kx, kres, kw;-imatrix
        if (&lt;mirrorwaves&gt; == 1) then
;            printsk &quot;\t\t%d\t%d\t%d\n&quot;, kres, kx, kw
            printsk &quot;\t\t%d\t%d\t%d\t   x\n&quot;, kx, kres, kw
            fprintks &quot;&lt;filename&gt;_&lt;matrix&gt;.matrxT&quot;, &quot;%d  %d  %d\n&quot;, kres, kx, kw;-imatrix
        endif
    od
    if kx= imatrix then
        fprintks &quot;&lt;filename&gt;_&lt;matrix&gt;.matrxT&quot;, &quot;&lt;/MATRIX&gt;\n&quot;                               ; closing quote
        turnoff
    endif
endif
;-------------------------------------------------------------------------------

break:
fprintks &quot;&lt;filename&gt;_&lt;matrix&gt;.matrxT&quot;, &quot;&lt;/MATRIX&gt;\n&quot;                            ; closing quote
turnoff
