            ixstart1 -=  &lt;X axis stretch1&gt;
            iystart1 -=  &lt;X axis stretch1&gt;
            while ixstart1 &lt; imatrix do   
                imaxx1   =   &lt;X axis max1&gt;         
                ixr1  random  1, imaxx1   
                                      
                if (&lt;direction1&gt; == 0) || (&lt;direction1&gt; == 2) then              ; diagonal or column Y
                    ixstart1 +=  ixr1 * &lt;X axis stretch1&gt;
                elseif (&lt;direction1&gt; == 1) then                                 ; column X
                    ixstart1 =  ixr1 * &lt;X axis stretch1&gt;
                endif
 
                imaxy1   =    &lt;Y axis max1&gt;
                iyr1  random  1, imaxy1
                if (&lt;direction1&gt; == 0) || (&lt;direction1&gt; == 1) then              ; diagonal or column Y
                    iystart1 +=  iyr1 * &lt;Y axis stretch1&gt;
                elseif (&lt;direction1&gt; == 2) then                                 ; column X
                    iystart1 =  iyr1 * &lt;Y axis stretch1&gt;
                endif

                if (ixstart1 &gt; imatrix || ixstart1 &gt; ixend1+1 || iystart1 &gt; imatrix || iystart1 &gt; iyend1+1) then
                    igoto   second                                               ; jump to next module
                endif

                imaxw11    =   &lt;Weight max1&gt;                                    ; Weight values (random)
                iminw11    =   &lt;Weight min1&gt;
                iw11  random  iminw11, imaxw11 
                imaxw12    =   &lt;Weight max1&gt;                                    ; Weight values (random)
                iminw12    =   &lt;Weight min1&gt;
                iw12  random  iminw12, imaxw12 

                prints &quot;\t\t%d\t%d\t%d\\n&quot;, ixstart1, iystart1, iw11            ; print to screen X Y W
                fprints &quot;&lt;filename&gt;_&lt;matrix&gt;.matrxT&quot;, &quot;%d   %d   %d \\n&quot;, ixstart1, iystart1, iw11  ; print values to file
                if (&lt;mirror1&gt; == 1 &amp;&amp; &lt;mirrorweight1&gt; == 1) then
                    fprints &quot;&lt;filename&gt;_&lt;matrix&gt;.matrxT&quot;, &quot;%d   %d   %d \\n&quot;, iystart1, ixstart1, iw12  ; print mirror values to file
                    prints &quot;\t\t%d\t%d\t%d\t   x\n&quot;, iystart1, ixstart1, iw12   ; and to screen 
                elseif (&lt;mirror1&gt; == 1 &amp;&amp; &lt;mirrorweight1&gt; == 0) then
                    iw12 =   iw11
                    fprints &quot;&lt;filename&gt;_&lt;matrix&gt;.matrxT&quot;, &quot;%d   %d   %d \\n&quot;, iystart1, ixstart1, iw12  ; print mirror values to file
                    prints &quot;\t\t%d\t%d\t%d\t   x\n&quot;, iystart1, ixstart1, iw12   ; and screen
                endif
            od

        elseif (&lt;LineCluster1&gt; == 0) then                                       ; set to CLUSTERS

            if (&lt;mirror1&gt; == 1) then
                prints &quot;\n\t       module 1: clusters mode, mirror ON\n&quot;, imatrix   ; print to screen clusters mode &amp; mirror mode
            else
                prints &quot;\n\t       module 1: clusters mode, mirror OFF\n&quot;, imatrix             
            endif
            prints &quot;\n\t\tX\tY\tW\tmirror?\n&quot;                                   ; to screen
            prints &quot;\t\t--\t--\t--\t-------\n&quot;                                  ; to screen

            iCntx1 = 0
            iCnty1 = 0
            while (ixstart1 &lt; imatrix) || (iystart1 &lt; imatrix) do
            if (ixstart1 &gt; imatrix || ixstart1 &gt; ixend1 || iystart1 &gt; imatrix || iystart1 &gt; iyend1) then
                igoto   second                                                   ; interrupt when bigger then matrix
            endif
            if (iCntx1 &lt;= (&lt;clustersize1&gt;-1)) then                              ; size of clusters
                imaxxC1   =   &lt;X axis maxC1&gt;                                    ; cluster X        
                ixrC1  random  1, imaxxC1
                imaxyC1   =   &lt;Y axis maxC1&gt;                                    ; cluster Y   
                iyrC1  random  1, imaxyC1
                imaxw11    =   &lt;Weight max1&gt;                                    ; Weight values (random)
                iminw11    =   &lt;Weight min1&gt;
                iw11  random  iminw11, imaxw11 
                imaxw12    =   &lt;Weight max1&gt;                                    ; Weight values (random)
                iminw12    =   &lt;Weight min1&gt;
                iw12  random  iminw12, imaxw12 

                fprints &quot;&lt;filename&gt;_&lt;matrix&gt;.matrxT&quot;, &quot;%d   %d   %d \\n&quot;, ixstart1, iystart1, iw11      ; print X, Y, W values to file
                prints &quot;\t\t%d\t%d\t%d\\n&quot;, ixstart1, iystart1, iw11            ; print to screen X Y W
                if (&lt;mirror1&gt; == 1 &amp;&amp; &lt;mirrorweight1&gt; == 1) then
                    fprints &quot;&lt;filename&gt;_&lt;matrix&gt;.matrxT&quot;, &quot;%d   %d   %d \\n&quot;, iystart1, ixstart1, iw12  ; print mirror values to file
                    prints &quot;\t\t%d\t%d\t%d\t   x\n&quot;, iystart1, ixstart1, iw12   ; and to screen 
                elseif (&lt;mirror1&gt; == 1 &amp;&amp; &lt;mirrorweight1&gt; == 0) then
                    iw12 =   iw11
                    fprints &quot;&lt;filename&gt;_&lt;matrix&gt;.matrxT&quot;, &quot;%d   %d   %d \\n&quot;, iystart1, ixstart1, iw12  ; print mirror values to file
                    prints &quot;\t\t%d\t%d\t%d\t   x\n&quot;, iystart1, ixstart1, iw12   ; and screen
                endif
                ixstart1 += 1                                                   ; jumps in all X values
                iCntx1   += 1
