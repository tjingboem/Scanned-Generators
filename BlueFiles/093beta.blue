                endif

                imaxw21    =   &lt;Weight max2&gt;                                    ; Weight values (random)
                iminw21    =   &lt;Weight min2&gt;
                iw21  random  iminw21, imaxw21 
                imaxw22    =   &lt;Weight max2&gt;                                    ; Weight values (random)
                iminw21    =   &lt;Weight min2&gt;
                iw22  random  iminw21, imaxw22 

                prints &quot;\t\t%d\t%d\t%d\\n&quot;, ixstart2, iystart2, iw21            ; print to screen X Y W
                fprints &quot;&lt;filename&gt;_&lt;matrix&gt;.matrxT&quot;, &quot;%d   %d   %d \\n&quot;, ixstart2, iystart2, iw21  ; print values to file
                if (&lt;mirror2&gt; == 1 &amp;&amp; &lt;mirrorweight2&gt; == 1) then
                    fprints &quot;&lt;filename&gt;_&lt;matrix&gt;.matrxT&quot;, &quot;%d   %d   %d \\n&quot;, iystart2, ixstart2, iw22  ; print mirror values to file
                    prints &quot;\t\t%d\t%d\t%d\t   x\n&quot;, iystart2, ixstart2, iw22   ; and to screen 
                elseif (&lt;mirror2&gt; == 1 &amp;&amp; &lt;mirrorweight2&gt; == 0) then
                    iw22 =   iw21
                    fprints &quot;&lt;filename&gt;_&lt;matrix&gt;.matrxT&quot;, &quot;%d   %d   %d \\n&quot;, iystart2, ixstart2, iw22  ; print mirror values to file
                    prints &quot;\t\t%d\t%d\t%d\t   x\n&quot;, iystart2, ixstart2, iw22   ; and screen
                endif
            od

        elseif (&lt;LineCluster2&gt; == 0) then                                       ; set to CLUSTERS

            if (&lt;mirror2&gt; == 1) then
                prints &quot;\n\t       module 2: clusters mode, mirror ON\n&quot;, imatrix   ; print to screen clusters mode &amp; mirror mode
            else
                prints &quot;\n\t       module 2: clusters mode, mirror OFF\n&quot;, imatrix             
            endif
            prints &quot;\n\t\tX\tY\tW\tmirror?\n&quot;                                   ; to screen
            prints &quot;\t\t--\t--\t--\t-------\n&quot;                                  ; to screen

            iCntx2 = 0
            iCnty2 = 0
            while (ixstart2 &lt; imatrix) || (iystart2 &lt; imatrix) do
            if (ixstart2 &gt; imatrix || ixstart2 &gt; ixend2 || iystart2 &gt; imatrix || iystart2 &gt; iyend2) then
                igoto   third                                                   ; interrupt when bigger then matrix
            endif
            if (iCntx2 &lt;= (&lt;clustersize2&gt;-1)) then                              ; size of clusters
                imaxxC2   =   &lt;X axis maxC2&gt;                                    ; cluster X        
                ixrC2  random  1, imaxxC2
                imaxyC2   =   &lt;Y axis maxC2&gt;                                    ; cluster Y   
                iyrC2  random  1, imaxyC2
                imaxw21    =   &lt;Weight max2&gt;                                    ; Weight values (random)
                iminw21    =   &lt;Weight min2&gt;
                iw21  random  iminw21, imaxw21 
                imaxw22    =   &lt;Weight max2&gt;                                    ; Weight values (random)
                iminw22    =   &lt;Weight min2&gt;
                iw22  random  iminw22, imaxw22 

                fprints &quot;&lt;filename&gt;_&lt;matrix&gt;.matrxT&quot;, &quot;%d   %d   %d \\n&quot;, ixstart2, iystart2, iw21      ; print X, Y, W values to file
                prints &quot;\t\t%d\t%d\t%d\\n&quot;, ixstart2, iystart2, iw21            ; print to screen X Y W
                if (&lt;mirror2&gt; == 1 &amp;&amp; &lt;mirrorweight2&gt; == 1) then
                    fprints &quot;&lt;filename&gt;_&lt;matrix&gt;.matrxT&quot;, &quot;%d   %d   %d \\n&quot;, iystart2, ixstart2, iw22  ; print mirror values to file
                    prints &quot;\t\t%d\t%d\t%d\t   x\n&quot;, iystart2, ixstart2, iw22   ; and to screen 
                elseif (&lt;mirror2&gt; == 1 &amp;&amp; &lt;mirrorweight2&gt; == 0) then
                    iw22 =   iw21
                    fprints &quot;&lt;filename&gt;_&lt;matrix&gt;.matrxT&quot;, &quot;%d   %d   %d \\n&quot;, iystart2, ixstart2, iw22  ; print mirror values to file
                    prints &quot;\t\t%d\t%d\t%d\t   x\n&quot;, iystart2, ixstart2, iw22   ; and screen
                endif
                ixstart2 += 1                                                   ; jumps in all X values
                iCntx2   += 1
                iystart2 += 1                                                   ; jumps in all Y values
                iCnty2  += 1
            else
                if (&lt;direction2&gt; == 0) || (&lt;direction2&gt; == 2) then
                    ixstart2 += &lt;skipX2&gt; * ixrC2                                ; create value jumps, even negative ones
                elseif (&lt;direction2&gt; == 1) then
                    ixstart2 = &lt;skipX2&gt; * ixrC2
                endif
                if (&lt;direction2&gt; == 0) || (&lt;direction2&gt; == 1) then
                    iystart2 += &lt;skipY2&gt; * iyrC2                                ; option for random values 
                elseif (&lt;direction2&gt; == 2) then
                    iystart2 = &lt;skipY2&gt; * iyrC2
                endif
                iCntx2 = 0
                iCnty2 = 0
            endif
        od                                                        
    endif
endif
;-------------------------------------------------------------------------------

;--------------------------module 3---------------------------------------------
third:
if (ixstart3 &gt; imatrix || iystart3 &gt; imatrix) then
    prints &quot;\nX or Y values too high!\n&quot;
endif

    if (&lt;module3&gt; == 1) then                                                    ; turn ON module 1

        if (&lt;LineCluster3&gt; == 1) then                                            ; set to LINES

            if (&lt;mirror3&gt; == 1) then
                prints &quot;\n\t       module 3: lines mode, mirror ON\n&quot;, imatrix           ; show lines mode &amp; mirror
            else
                prints &quot;\n\t       module 3: lines mode, mirror OFF\n&quot;, imatrix             
            endif
            prints &quot;\n\t\tX\tY\tW\tmirror?\n&quot;                                   ; to screen
            prints &quot;\t\t--\t--\t--\t-------\n&quot;                                  ; to screen

            if (ixstart3 &lt; 1 || iystart3 &lt; 1) then                              ; protection faulty user input
                prints &quot;\nstart value too small!\n\n&quot;
                igoto break
            endif

            ixstart3 -=  &lt;X axis stretch3&gt;
            iystart3 -=  &lt;X axis stretch3&gt;
            while ixstart3 &lt; imatrix do   
                imaxx3   =   &lt;X axis max3&gt;         
                ixr3  random  1, imaxx3   
                                      
                if (&lt;direction3&gt; == 0) || (&lt;direction3&gt; == 2) then              ; diagonal or column Y
                    ixstart3 +=  ixr3 * &lt;X axis stretch3&gt;
                elseif (&lt;direction3&gt; == 1) then                                 ; column X
                    ixstart3 =  ixr3 * &lt;X axis stretch3&gt;
                endif
 
                imaxy3   =    &lt;Y axis max3&gt;
                iyr3  random  1, imaxy3
                if (&lt;direction3&gt; == 0) || (&lt;direction3&gt; == 1) then              ; diagonal or column Y
                    iystart3 +=  iyr3 * &lt;Y axis stretch3&gt;
                elseif (&lt;direction3&gt; == 2) then                                 ; column X
                    iystart3 =  iyr3 * &lt;Y axis stretch3&gt;
                endif

                if (ixstart3 &gt; imatrix || ixstart3 &gt; ixend3+1 || iystart3 &gt; imatrix || iystart3 &gt; iyend3+1) then
                    igoto   randomlike                                          ; jump to next module
                endif

                imaxw31    =   &lt;Weight max3&gt;                                    ; Weight values (random)
                iminw31    =   &lt;Weight min3&gt;
                iw31  random  iminw31, imaxw31 
                imaxw32    =   &lt;Weight max3&gt;                                    ; Weight values (random)
                iminw32    =   &lt;Weight min3&gt;
                iw32  random  iminw32, imaxw32 

                prints &quot;\t\t%d\t%d\t%d\\n&quot;, ixstart3, iystart3, iw31            ; print to screen X Y W
                fprints &quot;&lt;filename&gt;_&lt;matrix&gt;.matrxT&quot;, &quot;%d   %d   %d \\n&quot;, ixstart3, iystart3, iw31  ; print values to file
                if (&lt;mirror3&gt; == 1 &amp;&amp; &lt;mirrorweight3&gt; == 1) then
                    fprints &quot;&lt;filename&gt;_&lt;matrix&gt;.matrxT&quot;, &quot;%d   %d   %d \\n&quot;, iystart3, ixstart3, iw32  ; print mirror values to file
                    prints &quot;\t\t%d\t%d\t%d\t   x\n&quot;, iystart3, ixstart3, iw32   ; and to screen 
                elseif (&lt;mirror3&gt; == 1 &amp;&amp; &lt;mirrorweight3&gt; == 0) then
                    iw32 =   iw31
                    fprints &quot;&lt;filename&gt;_&lt;matrix&gt;.matrxT&quot;, &quot;%d   %d   %d \\n&quot;, iystart3, ixstart3, iw32  ; print mirror values to file
                    prints &quot;\t\t%d\t%d\t%d\t   x\n&quot;, iystart3, ixstart3, iw32   ; and screen
                endif
            od

        elseif (&lt;LineCluster3&gt; == 0) then                                       ; set to CLUSTERS

            if (&lt;mirror3&gt; == 1) then
                prints &quot;\n\t       module 3: clusters mode, mirror ON\n&quot;, imatrix   ; print to screen clusters mode &amp; mirror mode
            else
                prints &quot;\n\t       module 3: clusters mode, mirror OFF\n&quot;, imatrix             
            endif
            prints &quot;\n\t\tX\tY\tW\tmirror?\n&quot;                                   ; to screen
            prints &quot;\t\t--\t--\t--\t-------\n&quot;                                  ; to screen

            iCntx3 = 0
            iCnty3 = 0
            while (ixstart3 &lt; imatrix) || (iystart3 &lt; imatrix) do
            if (ixstart3 &gt; imatrix || ixstart3 &gt; ixend2 || iystart3 &gt; imatrix || iystart3 &gt; iyend3) then
                igoto   randomlike                                              ; interrupt when bigger then matrix
            endif
            if (iCntx3 &lt;= (&lt;clustersize3&gt;-1)) then                              ; size of clusters
                imaxxC3   =   &lt;X axis maxC3&gt;                                    ; cluster X        
                ixrC3  random  1, imaxxC3
                imaxyC3   =   &lt;Y axis maxC3&gt;                                    ; cluster Y   
                iyrC3  random  1, imaxyC3
                imaxw31    =   &lt;Weight max3&gt;                                    ; Weight values (random)
                iminw31    =   &lt;Weight min3&gt;
                iw31  random  iminw31, imaxw31 
                imaxw32    =   &lt;Weight max3&gt;                                    ; Weight values (random)
                iminw32    =   &lt;Weight min3&gt;
                iw32  random  iminw32, imaxw32 

                fprints &quot;&lt;filename&gt;_&lt;matrix&gt;.matrxT&quot;, &quot;%d   %d   %d \\n&quot;, ixstart3, iystart3, iw31      ; print X, Y, W values to file
                prints &quot;\t\t%d\t%d\t%d\\n&quot;, ixstart3, iystart3, iw31            ; print to screen X Y W
                if (&lt;mirror3&gt; == 1 &amp;&amp; &lt;mirrorweight3&gt; == 1) then
                    fprints &quot;&lt;filename&gt;_&lt;matrix&gt;.matrxT&quot;, &quot;%d   %d   %d \\n&quot;, iystart3, ixstart3, iw32  ; print all different mirror values to file
                    prints &quot;\t\t%d\t%d\t%d\t   x\n&quot;, iystart3, ixstart3, iw32   ; and to screen 
                elseif (&lt;mirror3&gt; == 1 &amp;&amp; &lt;mirrorweight3&gt; == 0) then
                    iw32 =   iw31
                    fprints &quot;&lt;filename&gt;_&lt;matrix&gt;.matrxT&quot;, &quot;%d   %d   %d \\n&quot;, iystart3, ixstart3, iw32  ; print copied mirror values to file
                    prints &quot;\t\t%d\t%d\t%d\t   x\n&quot;, iystart3, ixstart3, iw32   ; and screen
                endif

                ixstart3 += 1                                                   ; jumps in all X values
                iCntx3   += 1
                iystart3 += 1                                                   ; jumps in all Y values
                iCnty3  += 1
            else
                if (&lt;direction3&gt; == 0) || (&lt;direction3&gt; == 2) then
                    ixstart3 += &lt;skipX3&gt; * ixrC3                                ; create value jumps, even negative ones
                elseif (&lt;direction3&gt; == 1) then
                    ixstart3 = &lt;skipX3&gt; * ixrC3
                endif
                if (&lt;direction3&gt; == 0) || (&lt;direction3&gt; == 1) then
                    iystart3 += &lt;skipY3&gt; * iyrC3                                ; option for random values 
                elseif (&lt;direction3&gt; == 2) then
                    iystart3 = &lt;skipY3&gt; * iyrC3
                endif
                iCntx3 = 0
                iCnty3 = 0
            endif
        od                                                        
    endif
endif
