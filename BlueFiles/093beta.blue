            <instrumentText>seed &lt;seed&gt;                                                                     ; common seed for tyrajectory
itrajectory =   &lt;trajectory&gt;          
imaxvalue   =   &lt;maximum value&gt;

;-------------------------------------------------------------------------------
prints &quot;\n\n\t   ---------------------------------------&quot;
prints &quot;\n\t   --SCANNED MATRIX TRAJECTORY GENERATOR--&quot;
prints &quot;\n\t\t&lt;filename&gt;_max&lt;maximum value&gt;_&lt;trajectory&gt;.traj\n&quot;                      ; print to screen only
prints &quot;\n\t\t  trajectory size = %d\n&quot;, itrajectory
prints &quot;\t   ---------------------------------------\n&quot;

;-------------------------------------------------------------------------------

;prints &quot;\n\nTRAJECTORY: &lt;filename&gt;_max&lt;maximum value&gt;_&lt;trajectory&gt;.traj is created (size = %d).\n&quot;, itrajectory   ; print to screen
kx init 0

if (&lt;generator type trajectory&gt; == 0) then
    prints &quot;\n\t\t     waves module:\n\n&quot;
    until kx = itrajectory do
        krfreq  loopxseg   kr/itrajectory, 0, 0, &lt;frequencymovement_slower&gt;, 0
        krdepth loopxseg   kr/itrajectory, 0, 0, &lt;depthmovement_less&gt;, 0
        kres   k_lfo8  &lt;frequency max&gt; * krfreq, &lt;depth max&gt; * krdepth, &lt;wave&gt; 
        kres    = (kres + 1)*.5
        kres    *= (imaxvalue)-1
  
        printks2 &quot;\t\t\t%d\n&quot;, kres
        fprintks &quot;&lt;filename&gt;_max&lt;maximum value&gt;_&lt;trajectory&gt;.traj&quot;, &quot;%d\n&quot;, kres
        kx += 1
    od

elseif (&lt;generator type trajectory&gt; == 1) then
    kx init 0
    prints &quot;\n\t\t    random module:\n\n&quot;
    until kx = itrajectory do 
        ktrig metro &lt;frequencyR&gt;
        kres   trandom ktrig, &lt;start value&gt;, &lt;max value&gt;
        if ( &lt;max value&gt; &gt; imaxvalue) then
            prints &quot;INPUT FOR MAXIMUM RANDOM VALUE IS TOO HIGH&quot;
        endif
        printks2 &quot;\t\t\t%d\n&quot;, kres
        fprintks &quot;&lt;filename&gt;_max&lt;maximum value&gt;_&lt;trajectory&gt;.traj&quot;, &quot;%d\n&quot;, kres
        kx += 1
    od
elseif (&lt;generator type trajectory&gt; == 2) then
    kx init 0
    prints &quot;\n\t\t    random hold module:\n&quot;
    until kx = itrajectory do 
        kres randomh 0, imaxvalue, &lt;frequencyR&gt;, 2, &lt;start value&gt;

        printks2 &quot;\t\t\t%d\n&quot;, kres
        fprintks &quot;&lt;filename&gt;_max&lt;maximum value&gt;_&lt;trajectory&gt;.traj&quot;, &quot;%d\n&quot;, kres
        kx += 1
    od
elseif (&lt;generator type trajectory&gt; == 3) then
kx init 0
    prints &quot;\n\t\t    random interpolate module 3:\n&quot;
    until kx = itrajectory do 
        kres randomi 0, imaxvalue, &lt;frequencyR&gt;, 2, &lt;start value&gt;

        printks2 &quot;\t\t\t%d\n&quot;, kres
        fprintks &quot;&lt;filename&gt;_max&lt;maximum value&gt;_&lt;trajectory&gt;.traj&quot;, &quot;%d\n&quot;, kres
        kx += 1
    od
elseif (&lt;generator type trajectory&gt; == 4) then                                  ; drawing mode
    kx init 0
    prints &quot;\n\t\t   drawing module:\n\n&quot;
    until kx = itrajectory do
        if (&lt;exponential&gt; == 1) then
            krfreq  loopxseg   kr/itrajectory, 0, 0, &lt;drawing_freehand&gt;, 0
        else
            krfreq  loopseg   kr/itrajectory, 0, 0, &lt;drawing_freehand&gt;, 0
        endif
        krfreq *=   imaxvalue

        printks2 &quot;\t\t\t%d\n&quot;, krfreq
        fprintks &quot;&lt;filename&gt;_max&lt;maximum value&gt;_&lt;trajectory&gt;.traj&quot;, &quot;%d\n&quot;, krfreq
        kx += 1
    od
endif

