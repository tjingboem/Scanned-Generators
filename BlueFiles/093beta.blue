            <instrumentText>imatrix =   &lt;matrix&gt;                                                            ; size of pattern line
seed &lt;seed&gt;                                                                     ; common seed for matrix

;-----------------------------X &amp; Y basics-------------------------------------- 
ixstart =   int(&lt;X axis start&gt; )                                                ; set start X point in matrix range
iystart =   int(&lt;Y axis start&gt; )                                                ; set start Y point in matrix range
ixend   =   int(imatrix * &lt;X axis end&gt;)                                         ; set last point realtive to matrix size for X axis
iyend   =   int(imatrix * &lt;Y axis end&gt;)                                         ; set last point realtive to matrix size for Y axis
ixstart1 =   int(&lt;X axis start1&gt; )                                              
iystart1 =   int(&lt;Y axis start1&gt; )                                                
ixend1   =   int(imatrix * &lt;X axis end1&gt;)                                       
iyend1   =   int(imatrix * &lt;Y axis end1&gt;)   
ixstart2 =   int(&lt;X axis start2&gt; )                                              
iystart2 =   int(&lt;Y axis start2&gt; )                                                
ixend2   =   int(imatrix * &lt;X axis end2&gt;)                                       
iyend2   =   int(imatrix * &lt;Y axis end2&gt;)   
ixstart3 =   int(&lt;X axis start3&gt; )                                              
iystart3 =   int(&lt;Y axis start3&gt; )                                                
ixend3   =   int(imatrix * &lt;X axis end3&gt;)                                       
iyend3   =   int(imatrix * &lt;Y axis end3&gt;)                                    

;-------------------------------------------------------------------------------
prints &quot;\n\n\t   ------------------------------------------&quot;
prints &quot;\n\t      --SCANNED MATRIX PATTERN GENERATOR--&quot;
;prints &quot;\n\t--------***************************--------&quot;
prints &quot;\n\t\t        &lt;filename&gt;_&lt;matrix&gt;.matrxT\n&quot;                      ; print to screen only
prints &quot;\n\t\t       matrix size = %d\n&quot;, imatrix
prints &quot;\t   ------------------------------------------\n&quot;
fprints &quot;&lt;filename&gt;_&lt;matrix&gt;.matrxT&quot;, &quot;&lt;MATRIX size=%d&gt;\\n&quot;, imatrix            ; open and send to file
;prints &quot;\n\t--------***************************--------\n&quot;
;-------------------------------------------------------------------------------

;--------------------------module 0---------------------------------------------
if (ixstart &gt; imatrix || iystart &gt; imatrix) then
    prints &quot;\nX or Y values too high!\n&quot;
endif

    if (&lt;module0&gt; == 1) then                                                    ; turn ON module 0
        if (&lt;LineCluster&gt; == 1) then                                            ; set to LINES

            if (&lt;mirror&gt; == 1) then
                prints &quot;\n\t       module 0: lines mode, mirror ON\n&quot;, imatrix   ; print to screen clusters mode &amp; mirror mode
            else
                prints &quot;\n\t       module 0: lines mode, mirror OFF\n&quot;, imatrix             
            endif
            prints &quot;\n\t\tX\tY\tW\tmirror?\n&quot;                                   ; to screen
            prints &quot;\t\t--\t--\t--\t-------\n&quot;                                  ; to screen

            if (ixstart &lt; 1 || iystart &lt; 1) then                                ; protection faulty user input
                prints &quot;\nstart value too small!\n\n&quot;
                igoto first
            endif

            ixstart -=  &lt;X axis stretch&gt;
            iystart -=  &lt;X axis stretch&gt;
            while ixstart &lt; imatrix do   
                imaxx   =   &lt;X axis max&gt;         
                ixr  random  1, imaxx   
                                      
                if (&lt;direction&gt; == 0) || (&lt;direction&gt; == 2) then                ; diagonal or column Y
                    ixstart +=  ixr * &lt;X axis stretch&gt;
                elseif (&lt;direction&gt; == 1) then                                  ; column X
                    ixstart =  ixr * &lt;X axis stretch&gt;
                endif
 
                imaxy   =    &lt;Y axis max&gt;
                iyr  random  1, imaxy
                if (&lt;direction&gt; == 0) || (&lt;direction&gt; == 1) then                ; diagonal or column Y
                    iystart +=  iyr * &lt;Y axis stretch&gt;
                elseif (&lt;direction&gt; == 2) then                                  ; column X
                    iystart =  iyr * &lt;Y axis stretch&gt;
                endif

                if (ixstart &gt; imatrix || ixstart &gt; ixend+1 || iystart &gt; imatrix || iystart &gt; iyend+1) then
                    igoto   first                                               ; jump to next module
                endif

                imaxw01    =   &lt;Weight max&gt;                                     ; Weight values (random)
                iminw01    =   &lt;Weight min&gt;
                iw01  random  iminw01, imaxw01 
                imaxw02    =   &lt;Weight max&gt;                                     ; Weight values (random)
                iminw02    =   &lt;Weight min&gt;
                iw02  random  iminw02, imaxw02 

                prints &quot;\t\t%d\t%d\t%d\\n&quot;, ixstart, iystart, iw01            ; print to screen X Y W
                fprints &quot;&lt;filename&gt;_&lt;matrix&gt;.matrxT&quot;, &quot;%d   %d   %d \\n&quot;, ixstart, iystart, iw01  ; print values to file
                if (&lt;mirror&gt; == 1 &amp;&amp; &lt;mirrorweight&gt; == 1) then
                    fprints &quot;&lt;filename&gt;_&lt;matrix&gt;.matrxT&quot;, &quot;%d   %d   %d \\n&quot;, iystart, ixstart, iw02  ; print mirror values to file
                    prints &quot;\t\t%d\t%d\t%d\t   x\n&quot;, iystart, ixstart, iw02   ; and to screen 
                elseif (&lt;mirror&gt; == 1 &amp;&amp; &lt;mirrorweight&gt; == 0) then
                    iw02 =   iw01
                    fprints &quot;&lt;filename&gt;_&lt;matrix&gt;.matrxT&quot;, &quot;%d   %d   %d \\n&quot;, iystart, ixstart, iw02  ; print mirror values to file
                    prints &quot;\t\t%d\t%d\t%d\t   x\n&quot;, iystart, ixstart, iw02   ; and screen
                endif
            od

        elseif (&lt;LineCluster&gt; == 0) then                                        ; set to CLUSTERS

            if (&lt;mirror&gt; == 1) then
                prints &quot;\n\t       module 0: clusters mode, mirror ON\n&quot;, imatrix   ; print to screen clusters mode &amp; mirror mode
            else
                prints &quot;\n\t       module 0: clusters mode, mirror OFF\n&quot;, imatrix             
            endif
            prints &quot;\n\t\tX\tY\tW\tmirror?\n&quot;                                   ; to screen
            prints &quot;\t\t--\t--\t--\t-------\n&quot;                                  ; to screen

            iCntx = 0
            iCnty = 0
            while (ixstart &lt; imatrix) || (iystart &lt; imatrix) do
            if (ixstart &gt; imatrix || ixstart &gt; ixend || iystart &gt; imatrix || iystart &gt; iyend) then
                igoto   first                                                   ; interrupt when bigger then matrix
            endif
            if (iCntx &lt;= (&lt;clustersize&gt;-1)) then                                ; size of clusters
                imaxxC   =   &lt;X axis maxC&gt;                                      ; cluster X        
                ixrC  random  1, imaxxC
                imaxyC   =   &lt;Y axis maxC&gt;                                      ; cluster Y   
                iyrC  random  1, imaxyC
                imaxw01    =   &lt;Weight max&gt;                                     ; Weight values (random)
                iminw01    =   &lt;Weight min&gt;
                iw01  random  iminw01, imaxw01 
                imaxw02    =   &lt;Weight max&gt;                                     ; Weight values (random)
                iminw02    =   &lt;Weight min&gt;
                iw02  random  iminw02, imaxw02 


                fprints &quot;&lt;filename&gt;_&lt;matrix&gt;.matrxT&quot;, &quot;%d   %d   %d \\n&quot;, ixstart, iystart, iw01      ; print X, Y, W values to file
                prints &quot;\t\t%d\t%d\t%d\\n&quot;, ixstart, iystart, iw01            ; print to screen X Y W
                if (&lt;mirror&gt; == 1 &amp;&amp; &lt;mirrorweight&gt; == 1) then
                    fprints &quot;&lt;filename&gt;_&lt;matrix&gt;.matrxT&quot;, &quot;%d   %d   %d \\n&quot;, iystart, ixstart, iw02  ; print mirror values to file
                    prints &quot;\t\t%d\t%d\t%d\t   x\n&quot;, iystart, ixstart, iw02   ; and to screen 
                elseif (&lt;mirror&gt; == 1 &amp;&amp; &lt;mirrorweight&gt; == 0) then
                    iw02 =   iw01
                    fprints &quot;&lt;filename&gt;_&lt;matrix&gt;.matrxT&quot;, &quot;%d   %d   %d \\n&quot;, iystart, ixstart, iw02  ; print mirror values to file
                    prints &quot;\t\t%d\t%d\t%d\t   x\n&quot;, iystart, ixstart, iw02   ; and screen
                endif
                ixstart += 1                                                    ; jumps in all X values
                iCntx   += 1
                iystart += 1                                                    ; jumps in all Y values
                iCnty  += 1
            else
                if (&lt;direction&gt; == 0) || (&lt;direction&gt; == 2) then
                    ixstart += &lt;skipX&gt; * ixrC                                   ; create value jumps, even negative ones
                elseif (&lt;direction&gt; == 1) then
                    ixstart = &lt;skipX&gt; * ixrC
                endif
                if (&lt;direction&gt; == 0) || (&lt;direction&gt; == 1) then
                    iystart += &lt;skipY&gt; * iyrC                                   ; option for random values 
                elseif (&lt;direction&gt; == 2) then
                    iystart = &lt;skipY&gt; * iyrC
                endif
                iCntx = 0
                iCnty = 0
            endif
        od                                                        
    endif
endif
;-------------------------------------------------------------------------------

;--------------------------module 1---------------------------------------------
first:
if (ixstart1 &gt; imatrix || iystart1 &gt; imatrix) then
    prints &quot;\nX or Y values too high!\n&quot;
endif

    if (&lt;module1&gt; == 1) then                                                    ; turn ON module 1
        if (&lt;LineCluster1&gt; == 1) then                                            ; set to LINES

            if (&lt;mirror1&gt; == 1) then
                prints &quot;\n\t       module 1: lines mode, mirror ON\n&quot;, imatrix   ; print to screen clusters mode &amp; mirror mode
            else
                prints &quot;\n\t       module 1: lines mode, mirror OFF\n&quot;, imatrix             
            endif
            prints &quot;\n\t\tX\tY\tW\tmirror?\n&quot;                                   ; to screen
