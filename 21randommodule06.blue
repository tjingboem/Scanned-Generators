            <globalOrc/>
            <globalSco/>
            <instrumentText>;---------------------initialize section----------------------------------------
imatrix =   &lt;matrix&gt;                                                            

ixstartR =   &lt;X axis startR&gt;                                                    ; X &amp; Y
iystartR =   &lt;Y axis startR&gt;

seed &lt;seed&gt;                                                                     ; in case of random, option to set seed

prints &quot;\n\n&lt;filename&gt;_&lt;matrix&gt;.matrxT is created (containing values not higher then %d) and has 3 columns:&quot;, imatrix   ; information
fprints &quot;&lt;filename&gt;_&lt;matrix&gt;.matrxT&quot;, &quot;&lt;MATRIX size=%d&gt;\\n&quot;, imatrix            ; opening quote to file
prints &quot;\n\n&lt;MATRIX size=%d&gt;\\n&quot;, imatrix                                       ; opening quote to screen

if (ixstartR &gt; imatrix || iystartR &gt; imatrix) then                              ; protection
    prints &quot;\nX or Y values too high!\n&quot;
endif

if (&lt;activateR&gt; == 1) then 
    while (ixstartR &lt; imatrix) || (iystartR &lt; imatrix) do    
            
        if  (ixstartR &gt; imatrix) || (iystartR &gt; imatrix) || (ixstartR &lt;= 0) || (iystartR &lt;= 0) then ; protection
            igoto break
        endif  
                             
        imaxw    =   &lt;WeightR max&gt;                                              ; weight  
        iw  random  1, imaxw 

        if (&lt;write&gt; == 1) then
            prints &quot;%d  %d  %d \\n&quot;, ixstartR, iystartR, iw
            fprints &quot;&lt;filename&gt;_&lt;matrix&gt;.matrxT&quot;, &quot;%d   %d   %d \\n&quot;, ixstartR, iystartR, iw  ; print values to file
        endif

        ix bexprnd &lt;X axis range&gt;
        iy bexprnd &lt;Y axis range&gt;
        ixstartR += ix+&lt;X axis direction&gt;
        iystartR += iy+&lt;Y axis direction&gt;
    od
endif

break:
fprints &quot;&lt;filename&gt;_&lt;matrix&gt;.matrxT&quot;, &quot;&lt;/MATRIX&gt;&quot;                               ; closing quote
prints &quot;&lt;/MATRIX&gt;\n\n\n&quot;
